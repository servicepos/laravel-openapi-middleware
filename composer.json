{
    "name": "c1st/laravel-openapi-middleware",
    "type": "library",
    "description": "Laravel OpenAPI Validation Middleware",
    "keywords": [
        "openapi",
        "validation",
        "middleware",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "C 1ST ApS",
            "email": "api@c1st.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "ckr/arraymerger": "^2.0|^3.0",
        "hkarlstrom/openapi-reader": "^0.5",
        "opis/json-schema": "^2.3",
        "respect/validation": "^1.1",
        "illuminate/http": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "laravel/framework": "^9.0|^10.0|^11.0|^12.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "pestphp/pest": "^2.36",
        "phpstan/phpstan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "C1st\\Middleware\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "C1st\\Middleware\\OpenApiValidation\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "analyse": "./vendor/bin/phpstan analyse -l 4 -c phpstan.neon src",
        "test": "./vendor/bin/pest"
    }
}
